{
  "name": "bananacat-bot",
  "version": "0.2.1",
  "description": "banana cat bot",
  "main": "index.js",
  "author": "lemon",
  "license": "WTFPL",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=development parcel build src/index.ts --target node --no-minify",
    "build:prod": "cross-env NODE_ENV=production parcel build src/index.ts --target node",
    "start": "npm run build && node dist/index.js",
    "start:prod": "npm run build:prod && node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "discord.js": "^11.5.1",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.146",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  }
}
